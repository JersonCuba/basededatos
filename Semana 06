SELECT titulo, autor
FROM libros l
WHERE EXISTS (
    SELECT 1
    FROM prestamos p
    WHERE p.id_libro = l.id_libro
);


SELECT titulo, autor
FROM libros l
WHERE NOT EXISTS (
    SELECT 1
    FROM prestamos p
    WHERE p.id_libro = l.id_libro
);


SELECT l.titulo, l.autor, p.id_prestamo, p.fecha_prestamo
FROM libros l
FULL JOIN prestamos p
ON l.id_libro = p.id_libro;


SELECT l.titulo, p.id_prestamo
FROM libros l
CROSS JOIN prestamos p;



SELECT titulo AS dato
FROM libros
UNION
SELECT CAST(id_prestamo AS VARCHAR) AS dato
FROM prestamos;
